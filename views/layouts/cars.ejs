<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>BRC Dashboard</title>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css" integrity="sha384-zCbKRCUGaJDkqS1kPbPd7TveP5iyJE0EjAuZQTgFLD2ylzuqKfdKlfG/eSrtxUkn" crossorigin="anonymous">
    
    <!-- Icon CSS -->
    <link href='https://unpkg.com/boxicons@2.1.2/css/boxicons.min.css' rel='stylesheet'>

    <!-- Custom CSS -->
    <link rel="stylesheet" href="/css/style.css">

</head>
<body>

    <div id="main-container" class="d-flex">

        <!-- SIDE MENU -->
        <div id="side-menu">

            <div class="main-nav text-center">
                <div class="main-nav__logo">
                    <a href="index.html">
                        <div class="logo"></div>
                    </a>
                </div>
                <div class="main-nav__item" >
                    <a href="#"><i class='bx bx-home-alt icon'></i><br>Dashboard</a>
                </div>
                <div class="main-nav__item">
                    <a href="#" class="cars"><i class='bx bxs-truck icon'></i><br>Cars</a>
                </div>
            </div>
        </div>
        <!-- END OF SIDE MENU -->
        
        <!-- MAIN -->
        <div id="main">
            
            <!-- ======= HEADER  ======= -->
            <div id="header" class="header">
                <div class="container-fluid">
                    <div class="row">

                        <div class="col-auto">
                            <div class="d-flex align-items-center">
                                <div class="col">
                                    <div class="logo-panjang"></div>
                                </div>
                            </div>
                        </div> <!-- End of Long Logo -->

                        <div class="col">
                            <div class="d-flex">
                                <div class="col-auto">
                                    <a href="#" class="menu-btn">
                                        <i class='bx bx-menu'></i>
                                    </a>
                                </div>
                            </div>
                        </div> <!-- End of Burger Menu -->

                        <div class="col-auto">
                            <div class="d-flex align-items-center">
                                <div class="col-auto">
                                    <div class="d-flex align-items-center">
                                        <form class="search-form d-flex align-items-center">
                                            <i class='bx bx-search'></i>
                                            <input type="text" placeholder="Search" class="search-input" title="Enter search keyword">
                                        </form>
                                        <button type="submit" title="Search" class="search-btn">Search</button>
                                    </div>
                                </div> <!-- End of Search Bar -->

                                <div class="col-auto">
                                    <div class="row">
                                        <div class="user-img">
                                            <div class="profile-pic rounded-circle d-flex  justify-content-center">
                                                <span class="initial-uname d-flex align-items-center justify-content-center">U</span>
                                            </div>
                                        </div>
                                        <div class="user-name d-flex align-items-center justify-content-center">
                                            <span class="uname">Unis Badri</span>
                                        </div>
                                        <div class="user-menu d-flex align-items-center justify-content-center">
                                            <div class="user-menu__dropdown">
                                                <button class="btn menu-btn" type="button" id="dropdown-menu" data-toggle="dropdown" aria-expanded="false">
                                                    <i class='bx bx-chevron-down bx-sm'></i>
                                                </button>
                                                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdown-menu">
                                                  <button class="dropdown-item" type="button">Action</button>
                                                  <button class="dropdown-item" type="button">Another action</button>
                                                  <button class="dropdown-item" type="button">Something else here</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>  <!-- End of User Header -->

                            </div>
                        </div> <!-- End of User Header -->

                    </div>
                </div>
            </div>
            <!-- ======= END OF HEADER ======= -->

            <!-- ======= CONTENT  ======= -->

            <div id="content">

                
                <div class="col">
                    <div class="row">
                        
                        <div class="right-menu">
                            <div class="col-auto">
                                <div class="side-menu">
                                    <div class="side-menu__title">
                                        <h5>CARS</h5>
                                    </div>
                                    <div class="side-menu__link">
                                        <a href="#">List Car</a>
                                    </div>
                                </div>
                            </div>
                        </div> <!-- End of Left Content -->

                        <div class="col">
                            <div id="right-content">
                                <div class="content-title">
                                    <nav>
                                        <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="#">Cars</a></li>
                                        <i class='bx bx-chevron-right'></i> 
                                        <li class="breadcrumb-item active">List Car</li>
                                        </ol>
                                    </nav>
                                    <h1>List Cars</h1>
                                </div><!-- End Page Title -->
                                
                                <div class="content-car">

                                    <div class="col">
                                        <div class="row">
                                            <div class="car-sorter">
                                                <div class="btn-sorter" role="toolbar">
                                                    <button type="button" class="btn btn-outline-secondary">All</button>
                                                    <button type="button" class="btn btn-outline-secondary">Small</button>
                                                    <button type="button" class="btn btn-outline-secondary">Medium</button>
                                                    <button type="button" class="btn btn-outline-secondary">Large</button>
                                                  </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="cars-card">
                                        <div class="col-12">
                                            <div class="row">

                                                <div class="col-4 card-group">
                                                    <div class="card" >
                                                        <img src="/img/car.png" class="card-img-top" alt="...">
                                                        <div class="card-body">
                                                            <p class="card-text mobil-pinjam">Nama/Tipe Mobil</p>
                                                            <h6 class="card-title harga-pinjam">Rp 430.000 / hari</h6>
                                                            <p class="card-text waktu-pinjam"><i class='bx bx-key'></i>&nbsp;Start rent - Finish rent</p>
                                                            <p class="card-text update-pinjam"><i class='bx bx-time-five'></i>&nbsp;Updated at 4 Apr 2022, 09.00</p>

                                                            <a href="#" class="btn btn-outline-danger"><i class='bx bx-trash-alt'></i>Delete</a>
                                                            <a href="#" class="btn btn-success"><i class='bx bx-edit'></i>Edit</a>
                                                        </div>
                                                      </div>
                                                </div>

                                                <div class="col-4 card-group">
                                                    <div class="card" >
                                                        <img src="/img/car.png" class="card-img-top" alt="...">
                                                        <div class="card-body">
                                                            <p class="card-text mobil-pinjam">Nama/Tipe Mobil</p>
                                                            <h6 class="card-title harga-pinjam">Rp 430.000 / hari</h6>
                                                            <p class="card-text waktu-pinjam"><i class='bx bx-key'></i>&nbsp;Start rent - Finish rent</p>
                                                            <p class="card-text update-pinjam"><i class='bx bx-time-five'></i>&nbsp;Updated at 4 Apr 2022, 09.00</p>

                                                            <a href="#" class="btn btn-outline-danger"><i class='bx bx-trash-alt'></i>Delete</a>
                                                            <a href="#" class="btn btn-success"><i class='bx bx-edit'></i>Edit</a>
                                                        </div>
                                                      </div>
                                                </div>

                                                <div class="col-4 card-group">
                                                    <div class="card" >
                                                        <img src="/img/car.png" class="card-img-top" alt="...">
                                                        <div class="card-body">
                                                            <p class="card-text mobil-pinjam">Nama/Tipe Mobil</p>
                                                            <h6 class="card-title harga-pinjam">Rp 430.000 / hari</h6>
                                                            <p class="card-text waktu-pinjam"><i class='bx bx-key'></i>&nbsp;Start rent - Finish rent</p>
                                                            <p class="card-text update-pinjam"><i class='bx bx-time-five'></i>&nbsp;Updated at 4 Apr 2022, 09.00</p>

                                                            <a href="#" class="btn btn-outline-danger"><i class='bx bx-trash-alt'>&nbsp;</i>Delete</a>
                                                            <a href="#" class="btn btn-success"><i class='bx bx-edit'></i>&nbsp;Edit</a>
                                                        </div>
                                                      </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>

                                </div>

                            </div>
                        </div> <!-- End of Right Content -->

                    </div>
                </div>

            </div> <!-- End of Main Content -->

            <!-- ======= END OF CONTENT ======= -->

        </div>
        <!-- END OF MAIN -->

    </div>

    
    <!-- Bootstrap JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-fQybjgWLrvvRgtW6bFlB7jaZrFsaBXjsOMm/tB9LTS58ONXgqbR9W8oWht/amnpF" crossorigin="anonymous"></script>

</body>
</html>