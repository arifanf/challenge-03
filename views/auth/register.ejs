<div class="login-container d-flex">

  <div class="img-side">
      <div class="login-img">
          <img src="/img/login.png" alt="" class="img-car">
      </div>
  </div> <!-- End of image cover -->

  <div class="form-side">
      <div class="col login-form">
          <div class="logo-panjang"></div>
          <h1>Registration Here</h1>
          <form action="/auth/register" method="POST">
              <div class="form-group">
                  <label for="username">Email</label>
                  <input type="text" class="form-control" id="username" aria-describedby="emailHelp" placeholder="Contoh: johndee@gmail.com" name="username" required>
              </div>
              <div class="form-group">
                  <label for="password1">Password</label>
                  <input type="password" class="form-control" id="password" placeholder="6+ karakter" name="password" required>
              </div>
              <button type="submit" class="btn btn-enter text-center">Sign Up</button>
              </form>
              <p class="register mt-1">Sudah punya akun? <a href="/auth/login">Login</a></p>
      </div>
  </div> <!-- End of form login -->

</div> <!-- End of Login Content -->